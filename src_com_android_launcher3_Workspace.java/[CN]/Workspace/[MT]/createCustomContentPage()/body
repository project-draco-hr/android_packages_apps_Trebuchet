{
  Log.d(TAG,"10249126 - createCustomContentPage()");
  CellLayout customScreen=(CellLayout)mLauncher.getLayoutInflater().inflate(R.layout.workspace_screen,null);
  Rect p=new Rect();
  AppWidgetHostView.getDefaultPaddingForWidget(mLauncher,mLauncher.getComponentName(),p);
  mWorkspaceScreens.put(CUSTOM_CONTENT_SCREEN_ID,customScreen);
  mScreenOrder.add(0,CUSTOM_CONTENT_SCREEN_ID);
  addFullScreenPage(customScreen);
  mDefaultPage++;
  setCurrentPage(getCurrentPage() + 1);
}
