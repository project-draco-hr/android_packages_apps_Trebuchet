{
  if (vacantCells == null) {
    return false;
  }
  if (this.spanX >= spanX && this.spanY >= spanY) {
    cellXY[0]=cellX;
    cellXY[1]=cellY;
    return true;
  }
  final ArrayList<VacantCell> list=vacantCells;
  final int count=list.size();
  for (int i=0; i < count; i++) {
    VacantCell cell=list.get(i);
    if (cell.spanX == spanX && cell.spanY == spanY) {
      cellXY[0]=cell.cellX;
      cellXY[1]=cell.cellY;
      return true;
    }
  }
  for (int i=0; i < count; i++) {
    VacantCell cell=list.get(i);
    if (cell.spanX >= spanX && cell.spanY >= spanY) {
      cellXY[0]=cell.cellX;
      cellXY[1]=cell.cellY;
      return true;
    }
  }
  return false;
}
