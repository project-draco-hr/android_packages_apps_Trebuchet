    @Override public void run(){
      Callbacks callbacks=tryGetCallbacks(oldCallbacks);
      if (callbacks != null) {
        callbacks.bindScreens(orderedScreens);
      }
    }
  }
;
  runOnMainThread(r,MAIN_THREAD_BINDING_RUNNABLE);
}
