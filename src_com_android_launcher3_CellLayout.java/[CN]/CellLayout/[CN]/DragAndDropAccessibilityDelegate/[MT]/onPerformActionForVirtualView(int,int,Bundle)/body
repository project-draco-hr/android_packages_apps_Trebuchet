{
  LauncherAccessibilityDelegate delegate=LauncherAppState.getInstance().getAccessibilityDelegate();
  if (delegate == null) {
    return false;
  }
  if (action == AccessibilityNodeInfoCompat.ACTION_CLICK) {
    String confirmation=getConfirmationForIconDrop(viewId);
    delegate.handleAccessibleDrop(CellLayout.this,getItemBounds(viewId),confirmation);
    return true;
  }
  return false;
}
