{
  mLauncher=launcher;
  mFolderIcon=folderIcon;
  if (sFolderOuterRingDrawable == null) {
    sFolderOuterRingDrawable=launcher.getResources().getDrawable(R.drawable.portal_ring_outer_holo);
  }
  if (sFolderInnerRingDrawable == null) {
    sFolderInnerRingDrawable=launcher.getResources().getDrawable(R.drawable.portal_ring_inner_holo);
  }
}
