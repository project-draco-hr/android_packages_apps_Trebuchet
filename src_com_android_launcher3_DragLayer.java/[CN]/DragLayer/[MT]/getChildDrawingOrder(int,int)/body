{
  if (true || LauncherAppState.isScreenLandscape(getContext()) || mWorkspaceIndex == -1 || mQsbIndex == -1 || mLauncher.getWorkspace().isDrawingBackgroundGradient()) {
    return i;
  }
  if (i == mQsbIndex) {
    return mWorkspaceIndex;
  }
 else   if (i == mWorkspaceIndex) {
    return mQsbIndex;
  }
 else {
    return i;
  }
}
