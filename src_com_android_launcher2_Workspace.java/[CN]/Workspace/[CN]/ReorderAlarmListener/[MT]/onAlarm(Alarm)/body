{
  int[] resultSpan=new int[2];
  mTargetCell=mDragTargetLayout.createArea((int)mDragViewVisualCenter[0],(int)mDragViewVisualCenter[1],minSpanX,minSpanY,spanX,spanY,child,mTargetCell,resultSpan,CellLayout.MODE_DRAG_OVER);
  mLastReorderX=mTargetCell[0];
  mLastReorderY=mTargetCell[1];
  if (mDragMode == DRAG_MODE_ADD_TO_FOLDER) {
  }
  mDragMode=DRAG_MODE_REORDER;
}
