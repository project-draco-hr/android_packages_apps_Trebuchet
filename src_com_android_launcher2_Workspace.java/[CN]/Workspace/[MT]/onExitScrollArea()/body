{
  if (mInScrollArea) {
    if (mDragTargetLayout != null) {
      mDragTargetLayout.setIsDragOverlapping(false);
      mDragTargetLayout=getCurrentDropLayout();
      mDragTargetLayout.onDragEnter();
      invalidate();
    }
    mPendingScrollDirection=DragController.SCROLL_NONE;
    mInScrollArea=false;
  }
}
