{
  mIconCache=cache;
  mIconCacheKey=info;
  mHolographicOutline=mIconCache.getOutline(mIconCacheKey);
  Bitmap icon;
  if (scaleUp) {
    icon=Bitmap.createScaledBitmap(info.iconBitmap,mScaledIconSize,mScaledIconSize,true);
  }
 else {
    icon=info.iconBitmap;
  }
  setCompoundDrawablesWithIntrinsicBounds(null,new FastBitmapDrawable(icon),null,null);
  setText(info.title);
  setTag(info);
}
