{
  if (DEBUG_DUMP_LOG) {
synchronized (sDumpLogs) {
      Log.d(TAG,"");
      Log.d(TAG,"*********************");
      Log.d(TAG,"Launcher debug logs: ");
      for (int i=0; i < sDumpLogs.size(); i++) {
        Log.d(TAG,"  " + sDumpLogs.get(i));
      }
      Log.d(TAG,"*********************");
      Log.d(TAG,"");
    }
  }
}
