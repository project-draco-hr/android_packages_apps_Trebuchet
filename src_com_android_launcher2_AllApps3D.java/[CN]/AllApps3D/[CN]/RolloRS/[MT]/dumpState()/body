  Log.d(TAG,"sRollo.mParams.bubbleHeight=" + mParams.bubbleHeight);
  Log.d(TAG,"sRollo.mParams.bubbleBitmapWidth=" + mParams.bubbleBitmapWidth);
  Log.d(TAG,"sRollo.mParams.bubbleBitmapHeight=" + mParams.bubbleBitmapHeight);
