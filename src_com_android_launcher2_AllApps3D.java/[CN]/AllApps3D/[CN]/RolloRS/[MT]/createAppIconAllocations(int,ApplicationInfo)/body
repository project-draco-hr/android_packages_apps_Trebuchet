{
  mIcons[index]=Allocation.createFromBitmap(sRS,item.iconBitmap,Allocation.MipmapControl.MIPMAP_ON_SYNC_TO_TEXTURE,Allocation.USAGE_GRAPHICS_TEXTURE);
  mLabels[index]=Allocation.createFromBitmap(sRS,item.titleBitmap,Allocation.MipmapControl.MIPMAP_ON_SYNC_TO_TEXTURE,Allocation.USAGE_GRAPHICS_TEXTURE);
}
