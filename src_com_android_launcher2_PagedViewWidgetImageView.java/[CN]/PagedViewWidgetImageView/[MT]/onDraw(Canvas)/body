{
  Insets insets=Insets.NONE;
  if (getBackground() != null) {
    insets=getBackground().getLayoutInsets();
  }
  canvas.save();
  canvas.clipRect(getScrollX() + getPaddingLeft() + insets.left,getScrollY() + getPaddingTop() + insets.top,getScrollX() + getRight() - getLeft() - getPaddingRight() - insets.right,getScrollY() + getBottom() - getTop() - getPaddingBottom() - insets.bottom);
  super.onDraw(canvas);
  canvas.restore();
}
