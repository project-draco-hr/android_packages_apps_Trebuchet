{
  if (mDrawIdentifier.equals(WORKSPACE_PORTRAIT) || mDrawIdentifier.equals(WORKSPACE_LANDSCAPE) || mDrawIdentifier.equals(ALLAPPS_PORTRAIT)|| mDrawIdentifier.equals(ALLAPPS_LANDSCAPE)) {
    int[] pos=getPunchThroughPosition();
    double diff=Math.sqrt(Math.pow(event.getX() - pos[0],2) + Math.pow(event.getY() - pos[1],2));
    if (diff < mRevealRadius) {
      if (mDrawIdentifier.equals(WORKSPACE_PORTRAIT)) {
      }
 else       if (mDrawIdentifier.equals(ALLAPPS_PORTRAIT)) {
      }
      return false;
    }
  }
 else   if (mDrawIdentifier.equals(FOLDER_PORTRAIT) || mDrawIdentifier.equals(FOLDER_LANDSCAPE)) {
    Folder f=mLauncher.getWorkspace().getOpenFolder();
    if (f != null) {
      Rect r=new Rect();
      f.getHitRect(r);
      if (r.contains((int)event.getX(),(int)event.getY())) {
        return false;
      }
    }
  }
  return true;
}
