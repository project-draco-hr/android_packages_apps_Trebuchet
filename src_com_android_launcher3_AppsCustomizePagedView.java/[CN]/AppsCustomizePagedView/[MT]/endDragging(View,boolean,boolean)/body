{
  if (isFlingToDelete || !success || (target != mLauncher.getWorkspace() && !(target instanceof DeleteDropTarget) && !(target instanceof Folder))) {
    mLauncher.exitSpringLoadedDragMode();
    mLauncher.unlockScreenOrientation(false);
  }
 else {
    mLauncher.unlockScreenOrientation(false);
  }
}
