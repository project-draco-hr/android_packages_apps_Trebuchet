{
  if (TAG_INCLUDE.equals(parser.getName())) {
    final int resId=getAttributeResourceValue(parser,ATTR_WORKSPACE,0);
    if (resId != 0) {
      return parseLayout(resId,screenIds);
    }
 else {
      return 0;
    }
  }
 else {
    return super.parseAndAddNode(parser,tagParserMap,screenIds);
  }
}
