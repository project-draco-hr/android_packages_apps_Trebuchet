{
  if (isCustomWidget) {
    return "WidgetProviderInfo(" + provider + ")";
  }
  return String.format("WidgetProviderInfo provider:%s package:%s short:%s label:%s span(%d, %d) minSpan(%d, %d)",provider.toString(),provider.getPackageName(),provider.getShortClassName(),getLabel(pm),spanX,spanY,minSpanX,minSpanY);
}
