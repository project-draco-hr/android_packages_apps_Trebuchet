{
  for (  AppEntry app : apps) {
    try {
      if (app.componentName == null || mIcons.containsKey(app.componentName.getPackageName())) {
        continue;
      }
      Drawable icon=mPackageManager.getApplicationIcon(app.componentName.getPackageName());
      mIcons.put(app.componentName.getPackageName(),icon);
      publishProgress();
    }
 catch (    PackageManager.NameNotFoundException e) {
    }
  }
  return null;
}
