{
synchronized (mCache) {
    final ResolveInfo resolveInfo=mPackageManager.resolveActivity(intent,0);
    ComponentName component=intent.getComponent();
    if (component == null) {
      return mDefaultIcon;
    }
    CacheEntry entry=cacheLocked(component,resolveInfo,null);
    if (title != null) {
      entry.title=title;
    }
    return entry.icon;
  }
}
