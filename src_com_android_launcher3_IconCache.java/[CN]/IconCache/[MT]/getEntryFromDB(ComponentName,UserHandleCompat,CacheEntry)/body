{
  Cursor c=mIconDb.getReadableDatabase().query(IconDB.TABLE_NAME,new String[]{IconDB.COLUMN_ICON,IconDB.COLUMN_LABEL},IconDB.COLUMN_COMPONENT + " = ? AND " + IconDB.COLUMN_USER+ " = ?",new String[]{component.flattenToString(),Long.toString(mUserManager.getSerialNumberForUser(user))},null,null,null);
  try {
    if (c.moveToNext()) {
      entry.icon=Utilities.createIconBitmap(c,0,mContext);
      entry.title=c.getString(1);
      if (entry.title == null) {
        entry.title="";
        entry.contentDescription="";
      }
 else {
        entry.contentDescription=mUserManager.getBadgedLabelForUser(entry.title,user);
      }
      return true;
    }
  }
  finally {
    c.close();
  }
  return false;
}
