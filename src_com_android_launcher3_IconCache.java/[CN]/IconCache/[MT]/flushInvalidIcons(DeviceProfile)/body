{
synchronized (mCache) {
    Iterator<Entry<CacheKey,CacheEntry>> it=mCache.entrySet().iterator();
    while (it.hasNext()) {
      final CacheEntry e=it.next().getValue();
      if ((e.icon != null) && (e.icon.getWidth() < grid.iconSizePx || e.icon.getHeight() < grid.iconSizePx)) {
        it.remove();
      }
    }
  }
}
