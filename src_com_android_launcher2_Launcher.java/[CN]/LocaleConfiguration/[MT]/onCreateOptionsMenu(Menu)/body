{
  if (isWorkspaceLocked()) {
    return false;
  }
  super.onCreateOptionsMenu(menu);
  menu.add(MENU_GROUP_WALLPAPER,MENU_WALLPAPER_SETTINGS,0,R.string.menu_wallpaper).setIcon(android.R.drawable.ic_menu_gallery).setAlphabeticShortcut('W');
  menu.add(0,MENU_MANAGE_APPS,0,R.string.menu_manage_apps).setIcon(android.R.drawable.ic_menu_manage).setAlphabeticShortcut('M');
  menu.add(0,MENU_HELP,0,R.string.menu_help).setIcon(android.R.drawable.ic_menu_help).setAlphabeticShortcut('H');
  return true;
}
