{
  super.onResume();
  mPaused=false;
  mUtsTestMode=SystemProperties.getInt("persist.sys.uts-test-mode",0) == 1;
  if (mRestoring) {
    mWorkspaceLoading=true;
    mModel.startLoader(this,true);
    mRestoring=false;
  }
  if (mIsNewIntent) {
    mWorkspace.post(new Runnable(){
      public void run(){
        stopSearch();
      }
    }
);
  }
  mIsNewIntent=false;
}
