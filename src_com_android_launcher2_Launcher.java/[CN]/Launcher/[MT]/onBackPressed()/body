{
  if (isAllAppsVisible()) {
    closeAllApps(true);
  }
 else   if (isCustomizationDrawerVisible()) {
    hideCustomizationDrawer();
    mWorkspace.unshrink();
  }
 else {
    closeFolder();
  }
  if (mPreviousView != null) {
    dismissPreview(mPreviousView);
    dismissPreview(mNextView);
  }
}
