{
  if (mPrevAlpha != alpha) {
    mPrevAlpha=alpha;
    mCachePaint.setAlpha(alpha);
    final Drawable[] dr=getCompoundDrawables();
    for (int i=0; i < dr.length; ++i) {
      if (dr[i] != null) {
        dr[i].mutate().setAlpha(alpha);
      }
    }
    super.onSetAlpha(alpha);
  }
  return true;
}
