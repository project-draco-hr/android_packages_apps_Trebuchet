{
  if (!isCancelled()) {
    if (cursor != null) {
      final LiveFolder folder=mFolder.get();
      if (folder != null) {
        final Launcher launcher=folder.mLauncher;
        folder.setContentAdapter(new LiveFolderAdapter(launcher,mInfo,cursor));
      }
    }
  }
 else   if (cursor != null) {
    cursor.close();
  }
}
