{
  if (info instanceof ItemInfo) {
    ItemInfo item=(ItemInfo)info;
    if (item.itemType == LauncherSettings.Favorites.ITEM_TYPE_APPWIDGET || item.itemType == LauncherSettings.Favorites.ITEM_TYPE_CUSTOM_APPWIDGET || item.itemType == LauncherSettings.Favorites.ITEM_TYPE_SHORTCUT) {
      return true;
    }
    if (item.itemType == LauncherSettings.Favorites.ITEM_TYPE_FOLDER) {
      return true;
    }
    if (item.itemType == LauncherSettings.Favorites.ITEM_TYPE_APPLICATION && item instanceof AppInfo) {
      AppInfo appInfo=(AppInfo)info;
      return (appInfo.flags & AppInfo.DOWNLOADED_FLAG) != 0;
    }
    if (item.itemType == LauncherSettings.Favorites.ITEM_TYPE_APPLICATION && item instanceof ShortcutInfo) {
      return true;
    }
  }
  return false;
}
