{
  if (mDragging) {
    mDragging=false;
    if (mOriginator != null) {
      mOriginator.setVisibility(View.VISIBLE);
    }
    for (    DragListener listener : mListeners) {
      listener.onDragEnd();
    }
    if (mDragView != null) {
      mDragView.remove();
      mDragView=null;
    }
  }
}
