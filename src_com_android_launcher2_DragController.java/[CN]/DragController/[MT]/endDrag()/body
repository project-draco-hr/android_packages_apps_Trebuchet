{
  if (mDragging) {
    mDragging=false;
    if (mOriginator != null) {
      mOriginator.setVisibility(View.VISIBLE);
    }
    for (    DragListener listener : mListeners) {
      listener.onDragEnd();
    }
    if (mDragObject.dragView != null) {
      mDragObject.dragView.remove();
      mDragObject.dragView=null;
    }
  }
}
