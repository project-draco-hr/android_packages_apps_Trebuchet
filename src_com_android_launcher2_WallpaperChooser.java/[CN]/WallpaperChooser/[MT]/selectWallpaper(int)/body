{
  if (mIsWallpaperSet) {
    return;
  }
  mIsWallpaperSet=true;
  try {
    WallpaperManager wpm=(WallpaperManager)getSystemService(WALLPAPER_SERVICE);
    wpm.setResource(mImages.get(position));
    setResult(RESULT_OK);
    finish();
  }
 catch (  IOException e) {
    Log.e(Launcher.LOG_TAG,"Failed to set wallpaper: " + e);
  }
}
