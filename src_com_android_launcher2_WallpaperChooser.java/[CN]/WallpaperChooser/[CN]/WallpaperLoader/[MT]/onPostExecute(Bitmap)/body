{
  if (b == null)   return;
  if (!isCancelled()) {
    if (mBitmap != null) {
      mBitmap.recycle();
    }
    final ImageView view=mImageView;
    view.setImageBitmap(b);
    mBitmap=b;
    final Drawable drawable=view.getDrawable();
    drawable.setFilterBitmap(true);
    drawable.setDither(true);
    view.postInvalidate();
    mLoader=null;
  }
 else {
    b.recycle();
  }
}
