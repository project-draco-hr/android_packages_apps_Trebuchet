{
  String value=null;
  try {
    JSONStringer json=new JSONStringer().object().key(KEY_STATE).value(info.state).key(KEY_PROGRESS).value(info.progress).endObject();
    value=json.toString();
  }
 catch (  JSONException e) {
    Log.e(TAG,"failed to serialize app state update",e);
  }
  return value;
}
