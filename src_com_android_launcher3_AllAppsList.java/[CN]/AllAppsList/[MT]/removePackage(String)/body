{
  final List<ApplicationInfo> data=this.data;
  for (int i=data.size() - 1; i >= 0; i--) {
    ApplicationInfo info=data.get(i);
    final ComponentName component=info.intent.getComponent();
    if (packageName.equals(component.getPackageName())) {
      removed.add(info);
      data.remove(i);
    }
  }
  mIconCache.flush();
}
