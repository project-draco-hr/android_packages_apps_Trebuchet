{
  Resources res=mLauncher.getResources();
  ArrayList<String> values=new ArrayList<String>(Arrays.asList(new String[]{res.getString(R.string.icon_labels),res.getString(R.string.app_drawer_style),res.getString(R.string.app_drawer_color),res.getString(R.string.fast_scroller),res.getString(R.string.fast_scroller_type),res.getString(R.string.home_screen_search_text)}));
  RemoteFolderManager.onInitializeDrawerSettings(values,mLauncher);
  String[] valuesArr=new String[values.size()];
  values.toArray(valuesArr);
  return valuesArr;
}
