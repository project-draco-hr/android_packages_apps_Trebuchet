{
  Resources res=mLauncher.getResources();
  ArrayList<String> values=new ArrayList<String>(Arrays.asList(new String[]{res.getString(R.string.home_screen_search_text),res.getString(R.string.icon_labels),res.getString(R.string.scrolling_wallpaper),res.getString(R.string.grid_size_text)}));
  boolean remoteAppsEnabled=SettingsProvider.getBoolean(mLauncher,null,R.bool.preferences_interface_homescreen_remote_folder_default);
  if (remoteAppsEnabled) {
    String remoteAppsName=RemoteFolderManager.getFeatureTitle(res);
    if (remoteAppsName != null) {
      values.add(remoteAppsName);
    }
  }
  String[] valuesArr=new String[values.size()];
  values.toArray(valuesArr);
  return valuesArr;
}
