{
  if (false) {
    Paint p=new Paint();
    p.setStyle(Paint.Style.FILL);
    p.setColor(0xaaffffff);
    canvas.drawRect(0,0,getWidth(),getHeight(),p);
  }
  if (getParent() != null) {
    if (mOnDrawRunnable != null) {
      mOnDrawRunnable.run();
      mOnDrawRunnable=null;
    }
  }
  canvas.drawBitmap(mBitmap,0.0f,0.0f,mPaint);
}
