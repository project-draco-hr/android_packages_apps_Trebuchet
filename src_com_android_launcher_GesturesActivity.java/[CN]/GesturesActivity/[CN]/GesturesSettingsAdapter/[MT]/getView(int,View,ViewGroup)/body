{
  if (position < FIXED_CHILDREN_COUNT) {
switch (position) {
case 0:
      convertView=createHeader(convertView,parent,R.string.gestures_group_settings);
    break;
case 1:
  convertView=createSetting(convertView,parent,R.string.gestures_preference_hotkey_title,R.string.gestures_preference_hotkey_summary);
break;
case 2:
convertView=createHeader(convertView,parent,R.string.gestures_group_gestures);
break;
}
return convertView;
}
return mAdapter.getView(position - FIXED_CHILDREN_COUNT,convertView,parent);
}
