{
  SharedPreferences sharedPrefs=mLauncher.getSharedPrefs();
  if (isClingsEnabled() && !sharedPrefs.getBoolean(FOLDER_CLING_DISMISSED_KEY,false) && !sharedPrefs.getBoolean(Launcher.USER_HAS_MIGRATED,false)) {
    Cling cling=initCling(R.id.folder_cling,R.id.cling_scrim,true,true);
    return cling;
  }
 else {
    removeCling(R.id.folder_cling);
    return null;
  }
}
