{
  boolean loaded;
synchronized (this) {
    loaded=mWorkspaceLoaded;
    mWorkspaceLoaded=true;
  }
  if (DEBUG_LOADERS)   Log.d(TAG,"loadAndBindWorkspace loaded=" + loaded);
  if (true || !loaded) {
    loadWorkspace();
    if (mStopped) {
      mWorkspaceLoaded=false;
      return;
    }
  }
  bindWorkspace();
}
