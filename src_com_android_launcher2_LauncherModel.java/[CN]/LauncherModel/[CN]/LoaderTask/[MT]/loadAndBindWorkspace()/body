{
  mIsLoadingAndBindingWorkspace=true;
  if (DEBUG_LOADERS) {
    Log.d(TAG,"loadAndBindWorkspace mWorkspaceLoaded=" + mWorkspaceLoaded);
  }
  if (!mWorkspaceLoaded) {
    loadWorkspace();
synchronized (LoaderTask.this) {
      if (mStopped) {
        return;
      }
      mWorkspaceLoaded=true;
    }
  }
  bindWorkspace();
}
