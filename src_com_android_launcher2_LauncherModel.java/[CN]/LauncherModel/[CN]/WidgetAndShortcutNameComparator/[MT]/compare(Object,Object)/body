{
  String labelA, labelB;
  if (mLabelCache.containsKey(a))   labelA=mLabelCache.get(a);
 else   labelA=(a instanceof AppWidgetProviderInfo) ? ((AppWidgetProviderInfo)a).label : ((ResolveInfo)a).loadLabel(mPackageManager).toString();
  if (mLabelCache.containsKey(b))   labelB=mLabelCache.get(b);
 else   labelB=(b instanceof AppWidgetProviderInfo) ? ((AppWidgetProviderInfo)b).label : ((ResolveInfo)b).loadLabel(mPackageManager).toString();
  return sCollator.compare(labelA,labelB);
}
