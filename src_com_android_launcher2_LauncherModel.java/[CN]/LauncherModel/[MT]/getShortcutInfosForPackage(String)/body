{
  ArrayList<ShortcutInfo> infos=new ArrayList<ShortcutInfo>();
  for (  ItemInfo i : sWorkspaceItems) {
    if (i instanceof ShortcutInfo) {
      ShortcutInfo info=(ShortcutInfo)i;
      if (packageName.equals(info.getPackageName())) {
        infos.add(info);
      }
    }
  }
  return infos;
}
