{
  final int countX=getCountX();
  final int countY=getCountY();
  pointToCellRounded(originX + (mCellWidth / 2),originY + (mCellHeight / 2),result);
  int rightOverhang=result[0] + spanX - countX;
  if (rightOverhang > 0) {
    result[0]-=rightOverhang;
  }
  result[0]=Math.max(0,result[0]);
  int bottomOverhang=result[1] + spanY - countY;
  if (bottomOverhang > 0) {
    result[1]-=bottomOverhang;
  }
  result[1]=Math.max(0,result[1]);
}
