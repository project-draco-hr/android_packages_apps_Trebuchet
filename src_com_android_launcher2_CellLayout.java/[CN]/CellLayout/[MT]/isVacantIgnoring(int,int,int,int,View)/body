{
  if (ignoreView != null) {
    markCellsAsUnoccupiedForView(ignoreView);
  }
  for (int i=0; i < spanY; i++) {
    if (!isRowEmpty(originY + i,originX,originX + spanX - 1,mOccupied)) {
      if (ignoreView != null) {
        markCellsAsOccupiedForView(ignoreView);
      }
      return false;
    }
  }
  if (ignoreView != null) {
    markCellsAsOccupiedForView(ignoreView);
  }
  return true;
}
