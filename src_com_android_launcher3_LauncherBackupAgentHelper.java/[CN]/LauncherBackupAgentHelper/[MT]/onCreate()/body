{
  boolean restoreEnabled=0 != Settings.Secure.getInt(getContentResolver(),SETTING_RESTORE_ENABLED,0);
  addHelper(LauncherBackupHelper.LAUNCHER_PREFS_PREFIX,new LauncherPreferencesBackupHelper(this,LauncherAppState.getSharedPreferencesKey(),restoreEnabled));
  addHelper(LauncherBackupHelper.LAUNCHER_PREFIX,new LauncherBackupHelper(this,restoreEnabled));
}
