{
  Intent preview=new Intent(WallpaperManager.ACTION_CHANGE_LIVE_WALLPAPER);
  preview.putExtra(WallpaperManager.EXTRA_LIVE_WALLPAPER_COMPONENT,mInfo.getComponent());
  a.onLiveWallpaperPickerLaunch();
  Utilities.startActivityForResultSafely(a,preview,WallpaperPickerActivity.PICK_LIVE_WALLPAPER);
}
