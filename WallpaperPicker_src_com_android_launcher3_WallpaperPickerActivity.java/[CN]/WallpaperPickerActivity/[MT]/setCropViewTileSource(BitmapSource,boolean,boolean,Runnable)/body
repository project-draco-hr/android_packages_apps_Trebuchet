{
  Runnable showPostExecuteRunnable=new Runnable(){
    @Override public void run(){
      if (postExecute != null) {
        postExecute.run();
      }
      setSystemWallpaperVisiblity(false);
    }
  }
;
  super.setCropViewTileSource(bitmapSource,touchEnabled,moveToLeft,showPostExecuteRunnable);
}
