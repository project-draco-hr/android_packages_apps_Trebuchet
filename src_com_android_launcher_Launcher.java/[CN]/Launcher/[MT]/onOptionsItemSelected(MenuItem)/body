{
switch (item.getItemId()) {
case MENU_ADD:
    addItems();
  return true;
case MENU_WALLPAPER_SETTINGS:
startWallpaper();
return true;
case MENU_SEARCH:
if (!mWorkspace.snapToSearch()) {
onSearchRequested();
}
return true;
case MENU_NOTIFICATIONS:
showNotifications();
return true;
}
return super.onOptionsItemSelected(item);
}
