{
  if (event.getAction() == KeyEvent.ACTION_DOWN) {
switch (event.getKeyCode()) {
case KeyEvent.KEYCODE_BACK:
      mWorkspace.dispatchKeyEvent(event);
    if (mDrawer.isOpened()) {
      closeDrawer();
    }
 else {
      closeFolder();
    }
  return true;
case KeyEvent.KEYCODE_HOME:
return true;
}
}
return super.dispatchKeyEvent(event);
}
