{
  Launcher launcher=mLauncher.get();
  if (launcher == null) {
    return;
  }
switch (msg.what) {
case MESSAGE_BIND_ITEMS:
{
      launcher.bindItems(this,mShortcuts,msg.arg1,msg.arg2);
      break;
    }
case MESSAGE_BIND_APPWIDGETS:
{
    launcher.bindAppWidgets(this,mAppWidgets,msg.arg1,msg.arg2);
    break;
  }
}
}
