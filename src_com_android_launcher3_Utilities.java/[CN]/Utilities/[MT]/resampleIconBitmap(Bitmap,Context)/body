{
synchronized (sCanvas) {
    if (sIconWidth == -1) {
      initStatics(context);
    }
    if (bitmap.getWidth() == sIconWidth && bitmap.getHeight() == sIconHeight) {
      return bitmap;
    }
 else {
      final Resources resources=context.getResources();
      return createIconBitmap(new BitmapDrawable(resources,bitmap),context);
    }
  }
}
